<!-- Start with navbar with "Scrape New" and "Saved Articles" buttons -->
<nav>
  <div class="nav-wrapper cyan darken-2">
    <a href="#!" class="brand-logo">GoodNewsNow</a>
    <a href="#" data-activates="mobile-demo" class="button-collapse">
      <i class="material-icons">menu</i>
    </a>
    <ul class="right hide-on-med-and-down">
      <li>
        <a class="waves-effect waves-green btn teal accent-3 scrape-new" id="scrape-new">New Articles
          <i class="material-icons right">library_add</i>
        </a>
      </li>
      <li>
        <a class="waves-effect waves-teal btn cyan lighten-1 view-saved" id="view-saved">Saved Articles
          <i class="material-icons right">collections_bookmark</i>
        </a>
      </li>
    </ul>
    <!-- Navbar when collapsed on mobile -->
    <ul class="side-nav" id="mobile-demo">
      <li>
        <a href="#" class="scrape-new">New Articles</a>
      </li>
      <li>
        <a href="#" class="view-saved">Saved Articles</a>
      </li>
    </ul>
  </div>
</nav>
<div class="container">
  <!-- Empty div for articles table to render dynamically on button clicks -->
  <div class="row">
    <div class="article-list center-align">
      {{#each Article}}
        {{#unless this.saved}}
          {{> new-block }}
        {{/unless}}
      {{/each}}
    </div>
  </div>
</div>
<!-- ^^Closes overall container div -->